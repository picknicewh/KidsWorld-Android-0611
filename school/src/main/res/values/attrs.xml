<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--圆形头像-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    <!--日历-->
    <declare-styleable name="DateView">
        <attr name="year" format="integer"/>
        <attr name="mouth" format="integer"/>
        <attr name="day" format="integer"/>
    </declare-styleable>
    <!--月份-->
    <declare-styleable name="MouthView">
        <attr name="my_year" format="integer"/>
        <attr name="my_mouth" format="integer"/>
    </declare-styleable>
    <!--多项选择日历-->
    <declare-styleable name="MulSelectDateView">
        <attr name="dv_year" format="integer"/>
        <attr name="dv_mouth" format="integer"/>
        <attr name="dv_day" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CircleDiskView">
        <attr name="cdv_part" format="integer"/>
        <attr name="cdv_circle_color" format="color"/>
    </declare-styleable>
</resources>