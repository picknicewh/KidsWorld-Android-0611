<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.hunme.user">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.UserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.UMassageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.USettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.UPhotoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.UploadPhotoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.AlbumSelectActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="net.hunme.login.UserChooseActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SystemInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.AdviceActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="net.hunme.baselibrary.activity.UpdateMessageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ProvsionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.InfoDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.AlbumDetailsActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="net.hunme.baselibrary.cordova.HMDroidGap"
            android:configChanges="orientation|keyboardHidden|navigation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustPan" />

        <activity android:name=".activity.MyDynamicActivity"></activity>
        <activity android:name=".activity.StatusDetilsActivity"></activity>
    </application>

</manifest>